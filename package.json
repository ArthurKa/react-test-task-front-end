{
  "name": "react-test-task-front-end",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "lint:check": "eslint --ext js,jsx,ts,tsx src",
    "lint:fix": "npm run lint:check -- --fix",
    "dev": "dotenv -e .env cross-env BROWSER=none react-scripts start",
    "build": "dotenv -e .env react-scripts build",
    "serve": "dotenv -e .env http-server build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prepare": "node -e \"try { require('husky').install(); } catch(e) { if(e.code !== 'MODULE_NOT_FOUND') throw e; }\""
  },
  "dependencies": {
    "dotenv-cli": "^6.0.0",
    "graphql": "^16.6.0",
    "graphql-request": "^4.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@arthurka/eslint": "^1.39.0",
    "@types/node": "^16.11.51",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.26",
    "cross-env": "^7.0.3",
    "http-server": "^14.1.1",
    "husky": "^8.0.1",
    "json5": "^2.2.1",
    "typescript": "^4.7.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
